    <queryString>
        <![CDATA[select distinct c.id,np.first_name || ' ' || np.last_name as insured_full_name,
       np.national_code as insured_national_code,
       sg.name as service_group_name,
       c.reception_date as claim_reception_date,
       c.total_amount as claim_total_amount,
       c.hcp_name as hcp_name,
       c.claim_number as claim_number,
        case
         when ec.order_json is null and ec.ep_inquiry_unique_code is null then
          'ندارد'
         when ec.order_json is not null or ec.ep_inquiry_unique_code is not null then
         fic.name || chr(13) || chr(10) || 'دارد'
       end as noteHeadEprscId,
       c.release_date as claim_release_date,
     ct.id as claim_Type_Id,
       coalesce(ca.total_payable_amount, 0) as total_payable_amount,
       (select coalesce(sum(cd.deduction_amount), 0)
          from hld.claim_deduction cd
          join hld.deduction_type dt
            on cd.type_id = dt.id
         where cd.claim_assessment_id = ca.id
           and dt.code = 'AUTO_EXTRA_CEILING'
           and cd.disable_date is null) as auto_deduction_amount,
       (select coalesce(sum(cd.deduction_amount), 0)
          from hld.claim_deduction cd
          join hld.deduction_type dt
            on cd.type_id = dt.id
         where cd.claim_assessment_id = ca.id
           and dt.code = 'AUTO_FRANCHISE'
           and cd.disable_date is null) as auto_faranchise_deduction_amount,
       coalesce(ca.total_deduction_amount, 0) as total_deduction_amount,
       cp.coverage_franchise_percent as coverage_franchise_percent,
      CASE WHEN dt.name IS NOT NULL THEN   dt.name  || ':' || cd.description  ELSE '-'  END as deduction_type_name
  from hld.claim c
  left join hld.claim_assessment ca
    on ca.id = c.id
     left join hld.extended_claim ec
    on ec.id = c.id
    join hld.claim_type ct
    on ct.id = c.claim_type_id
  join hld.claim_policy cp
    on cp.id = c.claim_policy_id
     join tpa.first_insurer_company fic
  on fic.id = cp.first_insurer_company_id
  join cor.natural_person np
    on np.id = cp.insured_id
  join tpa.service_group sg
    on sg.id = c.used_service_group_id
  join hld.claim_refer cr
    on cr.claim_id = c.id
  join hld.assessment_pack ap
    on ap.id = cr.assessment_pack_id
  left join hld.claim_deduction cd
    on cd.claim_assessment_id = ca.id and cd.disable_date is null
    and cd.type_id not in (9,11)
  left join hld.deduction_type dt
    on dt.id = cd.type_id
  where  ap.id = $P{P_ASSESSMENT_PACK_ID}
   order by c.id, c.reception_date]]>
    </queryString>
